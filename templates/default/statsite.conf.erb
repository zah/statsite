[statsite]
port = <%= @port %>
udp_port = <%= @port %>
log_level = <%= @loglevel %>
flush_interval = <%= @flush_interval %>
timer_eps = <%= @timer_eps %>
<% if @stream_command -%>
stream_cmd = <%= @stream_command %>
<% end -%>

<% if @histogram -%>
	<% @histogram.each do |section,opts| -%>
[<%= section.to_s %>]
		<% opts.each do |key,val| -%>
<%= key.to_s %>=<%= val.to_s %>
		<% end -%>
	<% end -%>

<% end -%>
